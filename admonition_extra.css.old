div.col-md-9 p.admonition-title:first-of-type {
    text-align: left;
}
.admonition-title {
    font-weight: bold;
    text-align: left;
    font-size: 150%;
}

.admonition.note {
    color: #3a87ad;
    background-color: #d9edf7;
    border-color: #bce8f1;
}
.admonition {
    padding: 15px;
    margin-bottom: 20px;
    border: 1px solid transparent;
    border-radius: 4px;
    text-align: left;
}